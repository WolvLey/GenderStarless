{
	"name": "Gender Star",
	"version": "1.0",
	"description": "Description",
	"browser_action": {
		"default_icon": "assets/lyricScope2.bmp"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
